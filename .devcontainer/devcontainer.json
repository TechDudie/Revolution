	"remoteUser": "vscode",
	"features": {
		"desktop-lite": {
			"password": "vscode",
			"webPort": "6080",
			"vncPort": "5901"
		}
	},
	"runArgs": ["--init", "--shm-size=1g"],
	"forwardPorts": [6080, 5901],
	"overrideCommand": false,
	"portsAttributes": {
		"3000": {
			"label": "VERCEL"
		},
		"4507": {
			"label": "HTTP SERVER"
		},
		"5901": {
			"label": "VNC"
		},
		"6080": {
			"label": "WEB VNC"
		}
	}
}